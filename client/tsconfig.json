{
	"compilerOptions": {
		"target": "es2020",
		"module": "ESNext",
		"lib": ["ESNext", "DOM"],
		"moduleResolution": "Node",
		// "strict": true,
		"sourceMap": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"noEmit": true,
		// "noUnusedLocals": true,
		// "noUnusedParameters": true,
		"noImplicitReturns": true,
		"jsx": "react-jsx",
		"skipLibCheck": true,
		"allowSyntheticDefaultImports": true
	},
	"include": ["./src"]
}
